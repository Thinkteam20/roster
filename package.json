{
  "name": "iffice",
  "productName": "IfficeRoster",
  "version": "1.0.0",
  "description": "iffice roster app",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@fullcalendar/adaptive": "^5.10.1",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@fullcalendar/resource-timegrid": "^5.10.1",
    "@fullcalendar/resource-timeline": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "file-saver": "^2.0.5",
    "fullcalendar-reactwrapper": "^1.0.7",
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.23",
    "mongodb": "^4.2.1",
    "mongoose": "^6.0.14",
    "nodemailer": "^6.7.2",
    "primeicons": "^5.0.0",
    "primereact": "^7.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
